<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ViewrecordEntity" table="viewrecord" schema="public" catalog="Unisle">
        <composite-id name="viewrecordEntityPK" class="entity.ViewrecordEntityPK">
            <key-property name="userid">
                <column name="userid" sql-type="varchar(30)" length="30"/>
            </key-property>
            <key-property name="articleid">
                <column name="articleid" sql-type="varchar(30)" length="30"/>
            </key-property>
            <key-property name="viewdatetime">
                <column name="viewdatetime" sql-type="timestamp"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>