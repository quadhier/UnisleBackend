<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ThemeEntity" table="theme" schema="public" catalog="Unisle">
        <composite-id name="themeEntityPK" class="entity.ThemeEntityPK">
            <key-property name="themename">
                <column name="themename" sql-type="varchar(30)" length="30"/>
            </key-property>
            <key-property name="boardname">
                <column name="boardname" sql-type="varchar(30)" length="30"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>