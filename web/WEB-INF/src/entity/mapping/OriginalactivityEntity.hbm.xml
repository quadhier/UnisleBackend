<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.OriginalactivityEntity" table="originalactivity" schema="public" catalog="Unisle">
        <id name="originalactivityid">
            <column name="originalactivityid" sql-type="varchar(30)" length="30"/>
        </id>
        <property name="content">
            <column name="content" sql-type="varchar(1000)" length="1000" />
        </property>
        <property name="attachment">
            <column name="attachment" sql-type="bytea" />
        </property>
        <property name="publisher">
            <column name="publisher" sql-type="varchar(30)" length="30" />
        </property>
        <property name="publicdatetime">
            <column name="publicdatetime" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="pros">
            <column name="pros" sql-type="smallint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>