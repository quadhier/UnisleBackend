<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.CollentionEntity" table="collention" schema="public" catalog="Unisle">
        <composite-id name="collentionEntityPK" class="entity.CollentionEntityPK">
            <key-property name="userid">
                <column name="userid" sql-type="varchar(30)" length="30"/>
            </key-property>
            <key-property name="articleid">
                <column name="articleid" sql-type="varchar(30)" length="30"/>
            </key-property>
        </composite-id>
        <property name="collectdatetime">
            <column name="collectdatetime" sql-type="timestamp" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>